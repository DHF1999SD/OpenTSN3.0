<?xml version="1.0" encoding="utf-8"?>

<nodes>
<node imac="2"> <!-- TSN交换机2 -->
<gate_table id="0">
<entry time_slot="0" state="255"/> <!-- state表示门控状态，使用bitmap形式，共8位，每一位代表一个队列的门控状态，1表示门控打开，0表示关闭 -->	
<entry time_slot="1" state="255"/>	
<entry time_slot="2" state="255"/>
<entry time_slot="3" state="255"/>	
</gate_table>

<gate_table id="1">
<entry time_slot="0" state="255"/>	
<entry time_slot="1" state="255"/>	
<entry time_slot="2" state="255"/>
<entry time_slot="3" state="255"/>
</gate_table>

<gate_table id="2">
<entry time_slot="0" state="255"/>	
<entry time_slot="1" state="255"/>	
<entry time_slot="2" state="255"/>	
<entry time_slot="3" state="255"/>
</gate_table>

<gate_table id="3">
<entry time_slot="0" state="255"/>	
<entry time_slot="1" state="255"/>	
<entry time_slot="2" state="255"/>
<entry time_slot="3" state="255"/>
</gate_table>

<register>
<time_slot>32</time_slot>    <!-- 单位us -->
<inject_slot_period>4</inject_slot_period>  
<qbv_or_qch>0</qbv_or_qch>
<rc_regulation_value>200</rc_regulation_value>
<be_regulation_value>300</be_regulation_value>
<unmap_regulation_value>200</unmap_regulation_value>
</register>

<forward_table>
<entry flowid="0" outport="4"/>  
<!-- outport使用bitmap形式，1表示向该端口转发，0表示不向该端口转发，如果多个位为1，则向多个端口转发-->   
<entry flowid="1" outport="8"/>    
<entry flowid="2" outport="256"/>  
<entry flowid="3" outport="2"/>    
<entry flowid="4" outport="2"/>    
<entry flowid="5" outport="2"/>    
<entry flowid="8" outport="8"/>    
<entry flowid="9" outport="2"/>   <!-- 摄像头流量转发 -->
<entry flowid="10" outport="256"/>
<entry flowid="11" outport="1"/>   <!-- 映射后的ST流量转发 -->
<entry flowid="12" outport="1"/>   <!-- 映射后的RC流量转发 -->
<entry flowid="13" outport="1"/>   <!-- 映射后的BE流量转发 -->

<entry flowid="20" outport="256"/>
<entry flowid="21" outport="8"/>  <!-- vlc流量转发 -->
<entry flowid="22" outport="8"/>  <!-- vlc流量转发 -->
<entry flowid="4096" outport="266"/>  <!-- 时间同步报文转发 -->
</forward_table>

</node>



<node imac="1">     <!-- TSN网卡3 摄像头视频流映射-->

<register>
<time_slot>32</time_slot>    <!-- 单位us -->
<inject_slot_period>4</inject_slot_period>  
<submit_slot_period>4</submit_slot_period>
<rc_regulation_value>200</rc_regulation_value>
<be_regulation_value>300</be_regulation_value>
<unmap_regulation_value>200</unmap_regulation_value>
</register>

<inject_table>
<entry time_slot="0" inject_addr="1"/>
<entry time_slot="1" inject_addr="2"/>
</inject_table>

<submit_table>
<entry time_slot="0" submit_addr="1"/>
<entry time_slot="1" submit_addr="2"/>
</submit_table>

<map_table>
<entry id="0"> <!-- 映射为flowid为11，类型为000的st流 -->
<src_ip>192.168.1.11</src_ip>			
<dst_ip>192.168.1.12</dst_ip>           
<src_port>1111</src_port>           
<dst_port>2222</dst_port>	        
<protocol_type>17</protocol_type>   							
<flow_type>0</flow_type>
<flow_id>11</flow_id>				
<inject_addr>1</inject_addr>        
<submit_addr>2</submit_addr> 
</entry>			
</map_table>	

<remap_table>
<entry id="0">
<flow_id>8</flow_id>
<dmac>bc:ba:c2:f3:f7:08</dmac>	<!-- 摄像头的MAC-->										
<outport>0</outport>        
</entry>	
<entry id="1">
<flow_id>9</flow_id>
<dmac>c0:04:80:00:00:00</dmac>  <!-- 摄像头接收端的MAC-->	  						
<outport>0</outport>        
</entry>

<entry id="2">
<flow_id>20</flow_id>
<dmac>e8:6a:64:c4:96:ff</dmac>	<!--vlc接收端的DMAC-->									
<outport>0</outport>        
</entry>	
<entry id="3">
<flow_id>22</flow_id>
<dmac>e8:6a:64:c4:96:ff</dmac>	<!--vlc接收端的DMAC-->
<outport>0</outport>        
</entry>
<entry id="4">
<flow_id>4308</flow_id>
<dmac>e8:6a:64:c4:96:ff</dmac>	<!--vlc接收端的DMAC-->
<outport>0</outport>        
</entry>

</remap_table>	

</node>
